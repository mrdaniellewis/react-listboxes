{
  "name": "@citizensadvice/react-combo-boxes",
  "version": "1.0.0",
  "private": true,
  "description": "A combo box implementations in React",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "scripts": {
    "build:cjs": "rm -rf cjs && babel src --out-dir cjs --source-maps --keep-file-extension",
    "build:es": "rm -rf es && babel src --out-dir es --source-maps --keep-file-extension --no-babelrc --presets=@babel/react",
    "prepare": "npm run build:cjs && npm run build:es",
    "install": "npm run prepare",
    "prebuild": "rm -rf dist",
    "build": "parcel build ./examples/index.pug --public-url .",
    "start": "parcel ./examples/index.pug",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "jest",
    "predeploy": "npm run lint",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gh-pages": "^2.2.0",
    "highlight.js": "^9.18.1",
    "jest": "^25.1.0",
    "marked": "^0.8.2",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-modules": "^1.5.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "sass": "^1.26.3"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "prop-types": ">= 15"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/runtime": "^7.8.7",
    "shallow-equal": "^1.2.1"
  },
  "sideEffects": false
}
